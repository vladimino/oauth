{% extends "base.twig" %}

{% block content %}
<div class="bs-docs-section" role="main">
    {% if error is defined %}
      <div class="row">
          <div class="col-lg-12">
              <div class="alert alert-dismissable alert-danger"><p>{{ error }}</p></div>
          </div>
      </div>
    {% endif %}

    <div class="row">
        {% block app_form %}{% endblock %}
    </div>
</div>
{% endblock %}
